{
  "name": "inflator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/backend",
    "packages/frontend",
    "packages/core-infra"
  ],
  "scripts": {
    "start-ui": "ENVIRONMENT=${ENVIRONMENT:-development} npm --prefix ./packages/frontend/ run start",
    "start-api": "ENVIRONMENT=${ENVIRONMENT:-development} npm --prefix ./packages/backend/ run start",
    "start": "ENVIRONMENT=development concurrently 'npm:start-*'",
    "start:local": "concurrently 'npm --prefix ./packages/frontend/ run start' 'npm --prefix ./packages/backend/ run start-express'",
    "test-ui": "npm --prefix ./packages/frontend/ run test",
    "test-api": "npm --prefix ./packages/backend/ run test",
    "build-api": "npm --prefix ./packages/backend/ run build",
    "test": "npm run test-ui && npm run test-api",
    "deploy-ui": "npm --prefix ./packages/frontend/ run deploy",
    "deploy-api": "npm --prefix ./packages/backend/ run deploy",
    "deploy": "npm run deploy-ui && npm run deploy-api",
    "init-tf": "ENVIRONMENT=${ENVIRONMENT:-development} npm --prefix ./packages/core-infra/ run init-tf:ci",
    "init-tf:nonprod": "npm --prefix ./packages/core-infra/ run init-tf:nonprod",
    "init-tf:prod": "npm --prefix ./packages/core-infra/ run init-tf:prod",
    "deploy-tf": "ENVIRONMENT=${ENVIRONMENT:-development} npm --prefix ./packages/core-infra/ run deploy-tf:ci",
    "deploy-tf:nonprod": "npm --prefix ./packages/core-infra/ run deploy-tf:nonprod",
    "deploy-tf:prod": "npm --prefix ./packages/core-infra/ run deploy-tf:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrdavenport/inflator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jrdavenport/inflator/issues"
  },
  "homepage": "https://github.com/jrdavenport/inflator#readme",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.135.0",
    "@aws-sdk/credential-providers": "^3.180.0",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@fontsource/roboto": "^4.5.7",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.5",
    "aws-lambda": "^1.0.7",
    "aws-rum-web": "^1.12.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "fast-csv": "^4.3.6",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.0",
    "styled-components": "^5.3.6",
    "tf": "npm:terraform@1.22.1",
    "tsc": "^2.0.4",
    "web-vitals": "^2.1.4",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@jest/types": "^28.1.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/aws-lambda": "^8.10.101",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "concurrently": "^7.2.2",
    "cors": "2.8.5",
    "jest": "^28.1.1",
    "nodemon": "^2.0.19",
    "react-scripts": "5.0.1",
    "serverless": "^3.19.0",
    "serverless-offline": "^8.8.0",
    "serverless-plugin-lambda-insights": "^1.6.0",
    "serverless-plugin-scripts": "^1.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "packageManager": "yarn@3.2.2"
}
